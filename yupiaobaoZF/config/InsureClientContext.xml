<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
    "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<!-- properties 	-->
	<bean id="propertyConfigurer" class="org.springframework.beans.factory.config.PropertyPlaceholderConfigurer">
		<property name="locations">
		<list>
		<value>classpath:InsureClientContext.properties</value>
		</list>
		</property>
	</bean>

	
	<bean id="policyScheduleDelegate" class="com.travelsky.tdp.insure.eterm.service.client.delegate.PolicyScheduleDelegateImpl">
		<!--  <property name="insureThubClient">
			<ref local="insureThubClient" />
		</property>-->
		<property name="insureHttpClient">
			<ref local="insureHttpClient" />
		</property>		
		<property name="transferClientType">
			<value>${transferImpl}</value>
		</property>
	</bean>


	<bean id="insureServiceDelegateTarget" class="com.travelsky.tdp.insure.eterm.service.client.servicedelegate.InsureServiceDelegateImpl">
		<property name="insureDelegate">
			<ref local="insureDelegate"/>
		</property>
	</bean>
	
	<bean id="insureHttpClient" class="com.travelsky.tdp.insure.eterm.service.client.InsureHttpClientImpl">
		<property name="proxySet">
			<value>${proxySet}</value>
		</property>
		<property name="proxyIP">
			<value>${proxyIP}</value>
		</property>
		<property name="proxyPort">
			<value>${proxyPort}</value>
		</property>
		<property name="urlStr">
			<value>${urlStr}</value>
		</property>
		<property name="urlStrbak1">
			<value>${urlStrbak1}</value>
		</property>
		<property name="urlStrbak2">
			<value>${urlStrbak2}</value>
		</property>
		<property name="urlStrbak3">
			<value>${urlStrbak3}</value>
		</property>
		
	</bean>
	<!-- 
	<bean id="insureThubClient" class="com.travelsky.tdp.insure.eterm.service.client.InsureThubClientImpl">
		<property name="application">
			<value>eids</value>
		</property>
		<property name="sessionID">
			<value></value>
		</property>
		<property name="encoding">
			<value></value>
		</property>
		<property name="password">
			<value></value>
		</property>		
		<property name="locale">
			<value></value>
		</property>		
		<property name="serialNo">
			<value></value>
		</property>	
		<property name="officeID">
			<value></value>
		</property>	
		<property name="userID">
			<value></value>
		</property>	
		<property name="role">
			<value></value>
		</property>	
		
	</bean>	
	 -->
	<bean id="insureDelegate" class="com.travelsky.tdp.insure.eterm.service.client.delegate.InsureDelegateImpl">
		<property name="policyScheduleDelegate">
			<ref local="policyScheduleDelegate" />
		</property>		
	</bean>
</beans>